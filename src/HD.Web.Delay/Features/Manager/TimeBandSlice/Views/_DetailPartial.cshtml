@{
	Layout = null;
}
@model TimeBandSliceViewModel


<form class="form-horizontal">
	<div class="form-group">
		<div class="col-md-4 control-label">
			<label asp-for="Name"></label>
		</div>
		<div class="col-md-8">
			<input asp-for="Name" class="form-control" type="text" />
		</div>
	</div>
	<div class="form-group">
		<div class="col-md-4 control-label">
			<label asp-for="TimeBandName"></label>
		</div>
		<div class="col-md-8">
			<input asp-for="TimeBandName" class="form-control" type="text" />
		</div>
	</div>
	<div class="form-group">
		<div class="col-md-4 control-label">
			<label asp-for="Description"></label>
		</div>
		<div class="col-md-8">
			<textarea asp-for="Description" class="form-control" type="text" rows="4"></textarea>
		</div>
	</div>
	<div class="form-group">
		<div class="col-md-4 control-label">
			<label asp-for="MaxDuration"></label>
		</div>
		<div class="col-md-8">
			<input asp-for="MaxDuration" class="form-control" type="text" />
		</div>
	</div>
</form>